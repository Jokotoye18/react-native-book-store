{
  "expo": {
    "name": "book-store",
    "slug": "book-store",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash-bookstore.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ademola.bookStore",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Book store needs to use Face ID / Touch ID to authenticate you"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ademola.bookStore"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
